    "editorLineNumber.foreground": "#a89f91",
    "editorLineNumber.activeForeground": "#6b645a",
    "editorCursor.foreground": "#2c2c2c",
    "editor.selectionBackground": "#d4cfc4",
    "editor.selectionHighlightBackground": "#e8e5dd",
    "editor.lineHighlightBackground": "#eae7df",
    "editor.findMatchBackground": "#ffd96a",
    "editor.findMatchHighlightBackground": "#ffe9a0",
    "editorWhitespace.foreground": "#d4cfc4",
    "editorIndentGuide.background": "#e0ddd5",
    "editorIndentGuide.activeBackground": "#c4bfb3",
    "editorBracketMatch.background": "#d4cfc4",
    "editorBracketMatch.border": "#a89f91",

    "sideBar.background": "#ebe8e0",
    "sideBar.foreground": "#2c2c2c",
