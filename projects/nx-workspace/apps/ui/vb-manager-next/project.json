{
  "name": "vb-manager-next",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/ui/vb-manager-next",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/ui/vb-manager-next"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/ui/vb-manager-next"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "vb-manager-next:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "vb-manager-next:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "vb-manager-next:build:production",
          "dev": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "pm2:start": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx build vb-manager-next --configuration=production",
          "pm2 start apps/ui/vb-manager-next/ecosystem.config.js"
        ],
        "parallel": false
      }
    },
    "pm2:stop": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pm2 stop vb-manager-next"
      }
    },
    "pm2:restart": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pm2 restart vb-manager-next"
      }
    },
    "pm2:delete": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pm2 delete vb-manager-next"
      }
    },
    "pm2:logs": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pm2 logs vb-manager-next"
      }
    },
    "pm2:status": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pm2 status"
      }
    }
  }
}
