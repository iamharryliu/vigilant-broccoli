{
  "name": "next-demo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/next-demo",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/next-demo"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/next-demo"
        },
        "production": {}
      }
    },
    "serve:vault": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "options": {
        "command": "ts-node scripts/fetch-secrets.ts 'nx serve next-demo'"
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "next-demo:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "next-demo:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "next-demo:build:production",
          "dev": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
