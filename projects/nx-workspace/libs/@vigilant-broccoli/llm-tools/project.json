{
  "name": "@vigilant-broccoli/llm-tools",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/@vigilant-broccoli/llm-tools/src",
  "projectType": "library",
  "release": {
    "version": {
      "currentVersionResolver": "git-tag",
      "preserveLocalDependencyProtocols": false,
      "manifestRootsToUpdate": ["dist/{projectRoot}"]
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/@vigilant-broccoli/llm-tools",
        "tsConfig": "libs/@vigilant-broccoli/llm-tools/tsconfig.lib.json",
        "packageJson": "libs/@vigilant-broccoli/llm-tools/package.json",
        "main": "libs/@vigilant-broccoli/llm-tools/src/index.ts",
        "assets": ["libs/@vigilant-broccoli/llm-tools/*.md"]
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
