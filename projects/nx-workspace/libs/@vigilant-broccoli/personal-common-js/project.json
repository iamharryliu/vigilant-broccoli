{
  "name": "@vigilant-broccoli/personal-common-js",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/@vigilant-broccoli/personal-common-js/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/@vigilant-broccoli/personal-common-js",
        "main": "libs/@vigilant-broccoli/personal-common-js/src/index.ts",
        "tsConfig": "libs/@vigilant-broccoli/personal-common-js/tsconfig.lib.json",
        "assets": []
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/@vigilant-broccoli/personal-common-js/jest.config.ts"
      }
    }
  }
}
