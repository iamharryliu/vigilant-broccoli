{
  "name": "@vigilant-broccoli/slack-bots",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/@vigilant-broccoli/slack-bots/src",
  "projectType": "library",
  "release": {
    "version": {
      "manifestRootsToUpdate": ["dist/{projectRoot}"],
      "currentVersionResolver": "git-tag",
      "fallbackCurrentVersionResolver": "disk"
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/@vigilant-broccoli/slack-bots",
        "tsConfig": "libs/@vigilant-broccoli/slack-bots/tsconfig.lib.json",
        "packageJson": "libs/@vigilant-broccoli/slack-bots/package.json",
        "main": "libs/@vigilant-broccoli/slack-bots/src/index.ts",
        "assets": ["libs/@vigilant-broccoli/slack-bots/*.md"]
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
