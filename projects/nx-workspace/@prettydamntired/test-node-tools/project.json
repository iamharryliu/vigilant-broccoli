{
  "name": "@prettydamntired/test-node-tools",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "@prettydamntired/test-node-tools/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/@prettydamntired/test-node-tools",
        "tsConfig": "@prettydamntired/test-node-tools/tsconfig.lib.json",
        "packageJson": "@prettydamntired/test-node-tools/package.json",
        "main": "@prettydamntired/test-node-tools/src/index.ts",
        "assets": ["@prettydamntired/test-node-tools/*.md"]
      }
    },
    "publish": {
      "command": "nx test @prettydamntired/test-node-tools && cd @prettydamntired/test-node-tools && npm version patch && cd ../../dist/@prettydamntired/test-node-tools && npm publish --access=public && cd ../../../",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "@prettydamntired/test-node-tools/jest.config.ts"
      }
    }
  },
  "tags": []
}
