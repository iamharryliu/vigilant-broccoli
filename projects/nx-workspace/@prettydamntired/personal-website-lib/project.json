{
  "name": "@prettydamntired/personal-website-lib",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "@prettydamntired/personal-website-lib/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/@prettydamntired/personal-website-lib",
        "main": "@prettydamntired/personal-website-lib/src/index.ts",
        "tsConfig": "@prettydamntired/personal-website-lib/tsconfig.lib.json",
        "assets": ["@prettydamntired/personal-website-lib/*.md"]
      }
    },
    "publish": {
      "command": "cd @prettydamntired/personal-website-lib && npm version patch && cd ../../dist/@prettydamntired/personal-website-lib && npm publish --access=public && cd ../../../",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "@prettydamntired/personal-website-lib/jest.config.ts"
      }
    }
  },
  "tags": []
}
